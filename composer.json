{
    "name": "sevavietl/kafka-rest-client",
    "description": "Asynchronous and synchronous http client for Kafka REST Proxy",
    "keywords": ["kafka", "kafka-rest-proxy", "asynchronous", "artax", "guzzle"],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "amphp/artax": "^3.0",
        "amphp/amp": "^2.0",
        "marc-mabe/php-enum": "^3.0",
        "yohang/finite": "^1.1",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.10.1",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "KafkaRestClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KafkaRestClient\\Test\\": "test"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "sevavietl",
            "email": "sevavietl@gmail.com"
        }
    ]
}
